<p id="notice"><%= notice %></p>

<h1>Conclusions</h1>

<table>
  <thead>
    <tr>
      <th>Ano</th>
      <th>Semestre</th>
      <th>Student</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @conclusions.each do |conclusion| %>
      <tr>
        <td><%= conclusion.ano %></td>
        <td><%= conclusion.semestre %></td>
        <td><%= conclusion.student %></td>
        <td><%= link_to 'Show', conclusion %></td>
        <td><%= link_to 'Edit', edit_conclusion_path(conclusion) %></td>
        <td><%= link_to 'Destroy', conclusion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Conclusion', new_conclusion_path %>
